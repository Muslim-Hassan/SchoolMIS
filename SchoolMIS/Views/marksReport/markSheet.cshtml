@model SchoolMIS.Models.marksModel

@{
    Layout = null;
}


<!DOCTYPE html>

<html dir="rtl">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>StudentsByClassReport</title>
    <link href="~/Content/custom/customCSS.css" rel="stylesheet" />
    <link href="~/Content/css/sb-admin-2.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.7.0.min.js"></script>
</head>
<body class="pashtoFont">

    <div class="container-fluid">

        <div class="col-12 d-flex flex-row justify-content-between  mb-3 p-3">

            <img src="~/Content/image/logo1.jpg" style="width:150px ;height:70px" />
            <div class="text-center">
                <h2><b>چاردیوال خصوصی ښوونځی</b></h2>
                <h4><b>د شاګردانو  دټولو نمرو  راپور  </b></h4>
            </div>
            <img src="~/Content/image/aei.jpg" style="width:100px;height:60px" />

        </div>
        @using (Html.BeginForm("markSheet", "marksReport", FormMethod.Post))
        {
            @Html.DropDownList("studentId", ViewBag.StudentList as SelectList, "شاګرد وټاکئ ", new { @class = "form-control hideInPrint" })

            <br />
            <input type="submit" value="د ټاکل شوی شاګرد نمری پیدا کړئ" class="btn btn-primary hideInPrint" />
        }
        <div class="row my-3">
            <div class="col-md-3 text-md-center  ">
                <p class="pe-2">
                    <strong>نوم:    </strong>@{
                        if (Model.Student != null)
                        {
                            @Model.Student.Name;
                        }
                    }

                </p>
            </div>
            <div class="col-md-3 text-md-center  ">
                <p>
                    <strong>پلار نوم :</strong> @{
                        if (Model.Student != null)
                        {
                            @Model.Student.FatherName;
                        }
                    }
                </p>
            </div>
            <div class="col-md-3 text-md-center  ">
                <p>
                    <strong>پلار نوم :</strong> @{
                        if (Model.Student != null)
                        {
                            @Model.Student.GrandFatherName;
                        }
                    }
                </p>
            </div>
            <div class="col-md-3 text-md-center   ">
                <p class="">
                    <strong>صنف:</strong> @{
                        if (Model.Student != null)
                        {
                            @Model.Student.ClassTable.Name;
                        }
                    }
                </p>

            </div>
        </div>
        @foreach (var attend in Model.Attendence)
        {
           
            switch (@attend.ClassTable.Name)
            {
                case "نرسری ټولګی":
                    ViewBag.prepFormalDays = @Model.Attendence.Where(f => f.ClassTable.Name == "نرسری ټولګی").Sum(f => f.FormalDays);
                    ViewBag.preppresent = @Model.Attendence.Where(f => f.ClassTable.Name == "نرسری ټولګی").Sum(f => f.Present);
                    break;
                case "اول ټولګی":
                    ViewBag.firstFormalDays = @Model.Attendence.Where(f => f.ClassTable.Name == "اول ټولګی").Sum(f => f.FormalDays);
                    ViewBag.firstpresent = @Model.Attendence.Where(f => f.ClassTable.Name == "اول ټولګی").Sum(f => f.Present);
                    break;
                case "دوهم ټولګی":
                    ViewBag.secondFormalDays = @Model.Attendence.Where(f => f.ClassTable.Name == "دوهم ټولګی").Sum(f => f.FormalDays);
                    ViewBag.secondpresent = @Model.Attendence.Where(f => f.ClassTable.Name == "دوهم ټولګی").Sum(f => f.Present);
                    break;
                case "دریم ټولګی":

                    ViewBag.thirdFormalDays = @Model.Attendence.Where(f => f.ClassTable.Name == "دریم ټولګی").Sum(f => f.FormalDays);
                    ViewBag.thirdpresent = @Model.Attendence.Where(f => f.ClassTable.Name == "دریم ټولګی").Sum(f => f.Present);
                    break;
                case "څلورم ټولګی":
                    ViewBag.fourthFormalDays = @Model.Attendence.Where(f => f.ClassTable.Name == "څلورم ټولګی").Sum(f => f.FormalDays);
                    ViewBag.fourthpresent = @Model.Attendence.Where(f => f.ClassTable.Name == "څلورم ټولګی").Sum(f => f.Present);
                    break;
                case "پنځم ټولګی":
                    ViewBag.fifthFormalDays = @Model.Attendence.Where(f => f.ClassTable.Name == "پنځم ټولګی").Sum(f => f.FormalDays);
                    ViewBag.fifthpresent = @Model.Attendence.Where(f => f.ClassTable.Name == "پنځم ټولګی").Sum(f => f.Present);
                    break;
                case "شپږم ټولګی":
                    ViewBag.sixthFormalDays = @Model.Attendence.Where(f => f.ClassTable.Name == "شپږم ټولګی").Sum(f => f.FormalDays);
                    ViewBag.sixthpresent = @Model.Attendence.Where(f => f.ClassTable.Name == "شپږم ټولګی").Sum(f => f.Present);
                    break;
                case "اووم ټولګی":
                    ViewBag.seventhFormalDays = @Model.Attendence.Where(f => f.ClassTable.Name == "اووم ټولګی").Sum(f => f.FormalDays);
                    ViewBag.seventhpresent = @Model.Attendence.Where(f => f.ClassTable.Name == "اووم ټولګی").Sum(f => f.Present);
                    break;
                case "اتم ټولګی":
                    ViewBag.eigthFormalDays = @Model.Attendence.Where(f => f.ClassTable.Name == "اتم ټولګی").Sum(f => f.FormalDays);
                    ViewBag.eigthpresent = @Model.Attendence.Where(f => f.ClassTable.Name == "اتم ټولګی").Sum(f => f.Present);
                    break;
                case "نهم ټولګی":
                    ViewBag.ninthFormalDays = @Model.Attendence.Where(f => f.ClassTable.Name == "نهم ټولګی").Sum(f => f.FormalDays);
                    ViewBag.ninthpresent = @Model.Attendence.Where(f => f.ClassTable.Name == "نهم ټولګی").Sum(f => f.Present);
                    break;
            }
        }

        @foreach (var item in Model.Score)
        {

            switch (@item.SubjectTable.Name)
            {
                case "قرانکریم":
                    switch (@item.ClassTable.Name)
                    {
                        case "نرسری ټولګی":
                            ViewBag.prepQuran = Model.Score.Where(f => f.ClassTable.Name == "نرسری ټولګی").Where(f => f.SubjectTable.Name == "قرانکریم").Sum(f => f.AbtainScore);
                            break;
                        case "اول ټولګی":
                            ViewBag.firstQuran = Model.Score.Where(f=>f.ClassTable.Name== "اول ټولګی").Where(f => f.SubjectTable.Name == "قرانکریم").Sum(f => f.AbtainScore);
                            break;
