                    }
                    break;
                case "ساینس":
                    switch (@item.ClassTable.Name)
                    {

                        case "څلورم ټولګی":
                            ViewBag.fourthScience = Model.Score.Where(f => f.ClassTable.Name == "څلورم ټولګی").Where(f => f.SubjectTable.Name == "ساینس").Sum(f => f.AbtainScore);
                            break;
                        case "پنځم ټولګی":
