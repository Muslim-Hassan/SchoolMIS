                            break;
                    }
                    break;
                case "دری":
                    switch (@item.ClassTable.Name)
                    {
                        case "نرسری ټولګی":
                            ViewBag.prepDari = Model.Score.Where(f => f.ClassTable.Name == "نرسری ټولګی").Where(f => f.SubjectTable.Name == "دری").Sum(f => f.AbtainScore);
                            break;
                        case "اول ټولګی":
                            ViewBag.firstDari = Model.Score.Where(f => f.ClassTable.Name == "اول ټولګی").Where(f => f.SubjectTable.Name == "دری").Sum(f => f.AbtainScore);
                            break;
                        case "دوهم ټولګی":
                            ViewBag.secondDari = Model.Score.Where(f => f.ClassTable.Name == "دوهم ټولګی").Where(f => f.SubjectTable.Name == "دری").Sum(f => f.AbtainScore); ;
                            break;
                        case "دریم ټولګی":
