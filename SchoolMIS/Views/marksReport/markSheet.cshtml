                            break;
                        case "څلورم ټولګی":
                            ViewBag.fourthDari = Model.Score.Where(f => f.ClassTable.Name == "څلورم ټولګی").Where(f => f.SubjectTable.Name == "دری").Sum(f => f.AbtainScore); ;
